    "express": "^4.21.2",
    "pg": "^8.13.3",
    "ts-fsrs": "^4.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.9",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.9",
    "supabase": "^2.15.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
