{
  "name": "card-api",
  "version": "1.0.0",
  "description": "REST API with TypeScript, Node.js, Express, PostgreSQL and Supabase auth",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:studio": "open http://127.0.0.1:54323",
    "setup": "node scripts/setup-dev.js",
    "seed:data": "node scripts/seed-data.js",
    "create:user": "node scripts/create-test-user.js",
    "create:decks": "node scripts/create-sample-decks.js",
    "test:login": "node scripts/login-test-user.js",
    "seed:users": "node scripts/seed-users.js"
  },
  "keywords": [],
